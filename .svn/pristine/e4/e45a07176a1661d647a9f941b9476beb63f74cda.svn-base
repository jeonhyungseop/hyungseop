@media ( max-width :1024px) {
	.btn_type {
		font-weight: 400
	}
	#container {
		width: auto;
		max-width: 768px
	}
	#content {
		width: 460px;
		margin: 0 auto
	}
	:root .input_chk label:before {
		width: 20px;
		height: 20px;
		background-size: 20px 20px
	}
	:root .terms_chk_all .input_chk label:before {
		background-size: 24px 24px
	}
	:root .input_chk input:checked+label:before {
		background-size: 20px 20px
	}
	:root .terms_chk_all .input_chk input:checked+label:before {
		background-size: 24px 24px
	}
}
@media ( max-width :768px) {
	#container {
		min-width: 220px;
		margin: 0 15px;
		box-sizing: border-box
	}
	#content {
		max-width: 460px;
		width: auto
	}
	.terms_chk_all .chk_all_txt br {
		display: none
	}
}
@media ( max-width :375px) {
	.terms_chk_all .chk_all_txt .m_br {
		display: block
	}
	.terms_chk_all .chk_all_txt br {
		display: none
	}
	.terms_chk_all input {
		margin-top: -9px
	}
}
@media ( max-width :320px) {
	.input_chk {
		margin-bottom: -3px
	}
}