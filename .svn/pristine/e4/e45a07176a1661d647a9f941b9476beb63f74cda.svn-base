@charset "UTF-8";
body, button, dd, dl, dt, fieldset, form, h1, h2, h3, h4, h5, h6, input,
	legend, li, ol, p, select, table, td, textarea, th, ul {
	margin: 0;
	padding: 0;
	-webkit-text-size-adjust: none
}
body, button, input, select, table, textarea {
	font-size: 12px;
	-webkit-text-size-adjust: none
}
html {
	font-size: 12px
}
body, html {
	width: 100%;
	height: 100%
}
button, input, select, textarea {
	border-radius: 0;
	border: none;
	background: 0 0;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	outline: 0;
	text-decoration: none;
	cursor: pointer;
	-webkit-text-size-adjust: none
}
button {
	cursor: pointer
}
label {
	cursor: pointer
}
fieldset, img {
	border: 0
}
ol, ul {
	list-style: none
}
address, em {
	font-style: normal
}
a {
	text-decoration: none;
	cursor: pointer
}
a:active, a:focus, a:hover {
	text-decoration: underline
}
table {
	border-collapse: collapse;
	border-spacing: 0;
	-webkit-text-size-adjust: none
}
hr {
	display: none
}
div{
	display : block;
}
img{
	margin-left: 40%;
	margin-right: 40%;
}
#container, #header{
	margin : 0 auto;
	max-width : 768px;
	min-width: 460px;
}
#header {
	position: relative;
	overflow: hidden;
	padding: 30px 0;
	box-sizing: border-box;
}
#header .h_logo {
	display: block;
	margin: 0 auto;
	width: 240px;
	background-repeat: no-repeat;
	background-position: 0 0;
	background-size: 240px auto;
	color: transparent;
	font-size: 0
}
.m_br{
	display : none;
}
.terms_bx_list {
	margin-top: 4px;
}
.terms_bx {
	display: block;
	padding-top: 30px;
}
.terms_box {
	position: relative;
    overflow: auto;
    height: 90px;
    margin-top: 8px;
    padding: 12px;
    border: 1px solid #dadada;
    background: #fff;
    box-sizing: border-box;
    scrollbar-base-color: #d9d9d9;
	scrollbar-arrow-color: #8e8e8e;
	scrollbar-3dlight-color: #d9d9d9;
	scrollbar-darkshadow-color: #d9d9d9;
	scrollbar-face-color: #d9d9d9;
	scrollbar-highlight-color: #d9d9d9;
	scrollbar-shadow-color: #d9d9d9;
	scrollbar-track-color: #f2f2f2;
	-webkit-overflow-scrolling: touch;
}
.terms_box .article{
	font_size : 12px;
	line-height: 16px;
	color : #666;
}
.terms_box::-webkit-scrollbar {
	overflow: visible;
	width: 12px;
	margin-right: 10px;
	background: 0 0;
}
.terms_box::-webkit-scrollbar-thumb {
	min-height: 20px;
	margin: 0 5px 0 0;
	-webkit-border-radius: 5px;
	background-color: rgba(45, 70, 97, .3);
}
.terms_box::-webkit-scrollbar-thumb:hover {
	background-color: rgba(45, 70, 97, .3);
}
.terms_box::-webkit-scrollbar-corner {
	background: 0 0;
}
.input_chk {
	position: relative;
	display: block;
	box-sizing: border-box;
}
.input_chk input {
	position: absolute;
	top: 50%;
	left: 1px;
	width: 18px;
	height: 18px;
	margin-top: -10px;
	outline: 0
}
.chk_all_txt{
	text-decoration: underline;
	/*word-break : 줄바꿈을 할때 단어기준으로 할지, 글자기준으로 할지 정하는 속성*/
    word-break: break-all;/*글자 기준으로 줄바꿈*/
}
.input_chk label {
	position: relative;
	display: block;
	width: 100%;
	padding-left: 29px;
	font-size: 14px;
	font-weight: 700;
	line-height: 21px;
	cursor: pointer;
	color: #333;
	box-sizing: border-box;
}
.input_chk a {
	color: #333;
}
.input_chk input:focus ~label {
	outline: dotted thin
}
.input_chk label .terms_choice {
	font-size: 12px;
	font-weight: bold;
	color: #435ebe;
}
.terms_necessary {
	font-size: 12px;
	font-weight: bold;
	display: inline-block;
	color: #435ebe;
}
.input_chk label .terms_no {
	color: #8e8e8e;
}
ul.terms_list {
	list-style: circle;
}
ul.terms_list li {
	padding-left: 10px;
	list-style-type: circle;
}
.terms_chk_all .input_chk label {
	padding-left: 34px;
	line-height: 21px;
}
.article, .policy_summary {
	font-size: 12px;
	line-height: 16px;
	color: #666;
}
.article {
	margin-top: 15px;
}
.article .article__title {
	padding-bottom: 8px;
}
.article__text+.article__text {
	margin-top: 8px;
}
.article__text a {
	color: #08a600;
}
.article__text em {
	color: #000;
}
.article__text__list {
	position: relative;
	padding-left: 20px;
	list-style : none;
}
.article__text__list:before {
	content: '';
	display: block;
	position: absolute;
	top: 7px;
	left: 8px;
	width: 2px;
	height: 2px;
	background: #8e8e8e;
}
.article__text__list+.article__text__list {
	margin-top: 5px;
}
.text_underline {
	text-decoration: underline;
}
.article:first-child {
	margin-top: 0;
}
.article .article__title, .clause .clause__title
	{
	font-size: 12px;
	font-weight: 700;
	line-height: 16px;
}
.clause .clause__title {
	margin-top: 10px;
}
.clause.large .clause__text, .clause.large .clause__title {
	font-size: 14px;
	font-weight: 700;
	line-height: 18px;
}
.sections{
	list-style : none;
}
.btn_area {
	margin: 30px 0 9px;
}
.btn_area.double {
	overflow: hidden;
	margin: 29px -5px 0;
}
.btn_area.double>span {
	display: block;
	float: left;
	width: 50%;
}
.btn_area.double span:before {
	content: '';
	display: block;
	clear: both;
}
.btn_area>span .btn_type {
	width: auto;
	margin: 0 5px;
	padding: 5% 10%;
}
.btn_type:active, .btn_type:focus, .btn_type:hover {
	text-decoration: none;
}
.btn_type+.btn_type {
	margin-top: 10px;
}
.btn_type {
    display: block;
    width: 50%;
    height : 75px;
    padding: 15px 0 15px;
    font-size: 18px;
    font-weight: 700;
    text-align: center;
    cursor: pointer;
    box-sizing: border-box;
    margin : 0 15px;
}
.btn_default {
	color: #fff;
	background-color: #8e8e8e;
}
.btn_primary {
	color: #fff;
	border: solid 1px rgba(0, 0, 0, .08);
	font-weight : bold;
	background-color: #826767;
}
.event_message {
	font-size : 12px;
	padding-top: 10px;
	color: #666;
}
.error{
	display:block;
	margin:8px 0 -1px;
	font-size:12px;
	line-height:14px;
	color:red
}
.join_title{
	margin : 19px 0;
	font-size : 14px;
	font-weight: 700;
	flex : 0 0 auto;
}
.ps_box {
	flex:0 0 auto;
}
.bir_wrap{
	dispaly : table;
}
.bir_dd, .bir_mm, .bir_yy {
	display: table-cell;
	table-layout: fixed;
	width: 147px;
	vertical-align: middle;
	*display: block;
	*float: left
}
.bir_mm+.bir_dd, .bir_yy+.bir_mm {
	padding-left: 10px
}
.error_next_box {
	display: block;
	margin: 9px 0 -2px;
	font-size: 12px;
	line-height: 14px;
	color: red
}
.green {
	color: #08a600 !important
}
